[submodule "crontab-dotbot"]
	path = .dotbot-crontab
	url = https://github.com/fundor333/crontab-dotbot.git
[submodule "dotbot-brew"]
	path = .dotbot-brew
	url = https://github.com/d12frosted/dotbot-brew.git
[submodule "dotbot-pip"]
	path = .dotbot-pip
	url = https://github.com/sobolevn/dotbot-pip.git
[submodule "zgenom"]
	path = .zgenom
	url = https://github.com/jandamm/zgenom.git
[submodule "zsh-quickstart-kit"]
	path = .zsh-quickstart-kit
	url = git@github.com:unixorn/zsh-quickstart-kit.git
[submodule "dotbot"]
	path = meta/.dotbot
	url = https://github.com/anishathalye/dotbot
	ignore = dirty
[submodule "ssh-private"]
	path = ssh
	url = git@github.com:duncz/ssh-private.git
[submodule "repos-private"]
	path = repos
	url = git@github.com:duncz/repos-private.git
[submodule ".dotbot-aptget"]
	path = .dotbot-aptget
	url = https://github.com/alegsan/dotbot-aptget.git
